		SELECT  diaobojilu.* FROM diaobojilu  diaobojilu 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.DiaobojiluView" >
		SELECT * FROM diaobojilu  diaobojilu <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
